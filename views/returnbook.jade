extends layout

block content
	- if(err)
		div.alert.alert-error= err
	- if(success)
		div.alert.alert-info= success
	div.borrow-main
		ul.borrow-list
			- if(list)
			- for ls in list
				li.item #{ls.username}&nbsp;&nbsp;#{ls.bookname}&nbsp;&nbsp;<a href="/book/check_return?id=#{ls._id}&isbn=#{ls.isbn}">确认还书</a>